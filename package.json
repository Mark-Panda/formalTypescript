{
    "name": "formaltypescript",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "migrate": "npx prisma migrate dev --schema=./schema/schema.prisma -n local --preview-feature",
        "generate": "npx prisma generate --schema=./schema/schema.prisma",
        "dev": "nodemon --watch './' -e ts --exec 'ts-node' ./lib/server.ts"
    },
    "nodemonConfig": {
        "ignore": [
            ".vscode/*",
            "public/*",
            "views/*",
            "*.json",
            "*.js"
        ]
    },
    "dependencies": {
        "@prisma/client": "^2.14.0",
        "body-parser": "^1.19.0",
        "cookie-parser": "~1.4.4",
        "debug": "~2.6.9",
        "ejs": "^3.1.5",
        "express": "~4.16.1",
        "http-status-codes": "^2.1.4",
        "js-yaml": "^4.0.0",
        "jsonwebtoken": "^8.5.1",
        "passport": "^0.4.1",
        "passport-jwt": "^4.0.0",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "@prisma/cli": "^2.14.0",
        "@types/express": "^4.17.11",
        "@types/js-yaml": "^4.0.0",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/node": "^14.14.21",
        "@types/passport": "^1.0.5",
        "@types/passport-jwt": "^3.0.3",
        "@typescript-eslint/eslint-plugin": "^4.14.0",
        "@typescript-eslint/parser": "^4.14.0",
        "eslint": "^7.18.0",
        "nodemon": "^2.0.7",
        "ts-node": "^9.1.1",
        "typescript": "^4.1.3"
    }
}