{
    "name": "formaltypescript",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "dev:serve": "nodemon ./bin/www",
        "dev:build": "npx tsc -w",
        "serve": "concurrently npm:dev:*",
        "deploy": "nodemon --watch './' -e ts --exec 'ts-node' ./lib/server.ts"
    },
    "nodemonConfig": {
        "ignore": [
            ".vscode/*",
            "public/*",
            "views/*",
            "*.json",
            "*.js"
        ]
    },
    "dependencies": {
        "body-parser": "^1.19.0",
        "cookie-parser": "~1.4.4",
        "debug": "~2.6.9",
        "ejs": "^3.1.5",
        "express": "~4.16.1",
        "js-yaml": "^4.0.0",
        "mongoose": "^5.11.12"
    },
    "devDependencies": {
        "@types/express": "^4.17.11",
        "@types/js-yaml": "^4.0.0",
        "@types/node": "^14.14.21",
        "@typescript-eslint/eslint-plugin": "^4.14.0",
        "@typescript-eslint/parser": "^4.14.0",
        "concurrently": "^5.3.0",
        "eslint": "^7.18.0",
        "nodemon": "^2.0.7",
        "ts-node": "^9.1.1",
        "typescript": "^4.1.3"
    }
}